{
  "builders": [
    {
      "communicator": "ssh",
      "source_path": "ubuntu/focal64",
      "provider": "virtualbox",
      "add_force": true,
      "template": "Vagrantfile",
      "type": "vagrant"
    }
  ],
  "provisioners": [
    {
      "type": "ansible",
      "playbook_file": "./ansible/playbook.yml",
      "extra_arguments": [ "-v" ]
    }
  ]
}
